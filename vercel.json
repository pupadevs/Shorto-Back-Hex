{
    "version": 2,
    "builds": [
      {
        "src": "artisan migrate --force --database=mysql",
        "use": "@vercel/php@8.2"
      },
      {
        "src": "artisan migrate --force --database=mysql_read",
        "use": "@vercel/php@8.2"
      },
      {
        "src": "artisan key:generate",
        "use": "@vercel/php@8.2"
      }
    ],
    "functions": {
      "api/index.php": {
        "runtime": "@vercel/php@8.2"
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.php"
      }
    ],
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "true",
      "APP_URL": "https://yourproductionurl.com",
      "APP_CONFIG_CACHE": "/tmp/config.php",
      "APP_EVENTS_CACHE": "/tmp/events.php",
      "APP_PACKAGES_CACHE": "/tmp/packages.php",
      "APP_ROUTES_CACHE": "/tmp/routes.php",
      "APP_SERVICES_CACHE": "/tmp/services.php",
      "VIEW_COMPILED_PATH": "/tmp",
      "CACHE_DRIVER": "array",
      "LOG_CHANNEL": "stderr",
      "SESSION_DRIVER": "cookie"
    }
  }
  